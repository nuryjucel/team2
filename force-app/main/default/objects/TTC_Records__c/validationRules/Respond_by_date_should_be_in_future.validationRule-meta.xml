<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Respond_by_date_should_be_in_future</fullName>
    <active>true</active>
    <errorConditionFormula>AND (Respond_by_Date__c &lt; TODAY()+5 , isNew())</errorConditionFormula>
    <errorMessage>Respond by date should be in future and minimum 5 days interval from today</errorMessage>
</ValidationRule>
